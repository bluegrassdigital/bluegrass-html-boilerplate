{
  "name": "bluegrass-html-boilerplate",
  "version": "2.2.0",
  "description": "Bluegrass front-end boilerplate - gulp, webpack, babel, blue-widgets, Sass, autoprefixer, ITCSS, BEM, Nunjucks, browsersync",
  "author": {
    "name": "Simon Buerger",
    "url": "https://github.com/simonbuerger"
  },
  "scripts": {
    "lint-js": "semistandard ./source/scripts/**/*.js",
    "lint-scss": "sass-lint -c ./.sass-lint.yml ./source/styles/**/*.scss -v",
    "lint-html": "htmlhint ./dist/pages/**/*.html",
    "lint": "npm run lint-js && npm run lint-scss && npm run lint-html"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.0",
    "@babel/register": "^7.4.0",
    "autoprefixer": "^9.4.8",
    "babel-loader": "^8.0.5",
    "del": "^3.0.0",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-data": "^1.3.1",
    "gulp-favicons": "^2.3.0",
    "gulp-notify": "^3.2.0",
    "gulp-nunjucks-html": "^2.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-util": "^3.0.8",
    "gulp-wait": "^0.0.2",
    "htmlhint": "^0.10.1",
    "npm-sass": "^2.3.0",
    "nunjucks-browsersync-server": "^2.0.0",
    "require-dir": "^1.2.0",
    "sass-lint": "^1.12.1",
    "semistandard": "^13.0.1",
    "webpack": "^4.29.5"
  }
}
