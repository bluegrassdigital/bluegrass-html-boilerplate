{
  "name": "bluegrass-html-boilerplate",
  "version": "2.2.0",
  "description": "Bluegrass front-end boilerplate - gulp, webpack, babel, blue-widgets, Sass, autoprefixer, ITCSS, BEM, Nunjucks, browsersync",
  "author": {
    "name": "Simon Buerger",
    "url": "https://github.com/simonbuerger"
  },
  "scripts": {
    "lint-js": "standard ./source/scripts/**/*.js",
    "lint-scss": "sass-lint -c ./.sass-lint.yml ./source/styles/**/*.scss -v",
    "lint-html": "htmlhint ./dist/pages/**/*.html",
    "lint": "npm run lint-js && npm run lint-scss && npm run lint-html"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "del": "^3.0.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-changed": "^3.1.0",
    "gulp-data": "^1.2.1",
    "gulp-favicons": "^2.2.7",
    "gulp-notify": "^3.0.0",
    "gulp-nunjucks-html": "^2.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-util": "^3.0.8",
    "gulp-wait": "^0.0.2",
    "htmlhint": "^0.9.13",
    "npm-sass": "^2.2.1",
    "nunjucks-browsersync-server": "^2.0.0",
    "require-dir": "^1.0.0",
    "run-sequence": "^2.2.0",
    "sass-lint": "^1.12.1",
    "standard": "^12.0.1",
    "webpack": "^4.17.1"
  }
}
